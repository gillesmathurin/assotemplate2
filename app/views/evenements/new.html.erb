<h1>New evenement</h1>

<% form_for(@evenement) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :date %><br />
    <%= f.date_select :date %>
  </p>
  <p>
    <%= f.label :h_debut %><br />
    <%= f.datetime_select :h_debut %>
  </p>
  <p>
    <%= f.label :h_fin %><br />
    <%= f.datetime_select :h_fin %>
  </p>
  <p>
    <%= f.label :lieu %><br />
    <%= f.text_field :lieu %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :picture_file_name %><br />
    <%= f.text_field :picture_file_name %>
  </p>
  <p>
    <%= f.label :picture_content_type %><br />
    <%= f.text_field :picture_content_type %>
  </p>
  <p>
    <%= f.label :picture_file_size %><br />
    <%= f.text_field :picture_file_size %>
  </p>
  <p>
    <%= f.label :picture_updated_at %><br />
    <%= f.datetime_select :picture_updated_at %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', evenements_path %>